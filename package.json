{
  "name": "chrome-extension-options",
  "version": "0.1.6",
  "description": "Easily set up an options page for a Chrome Extension using React",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --config webpack.dev.config.js --progress --colors --port 8090",
    "build": "npm run clean-build && webpack --config webpack.dev.config.js",
    "compile": "npm run clean-lib && webpack",
    "clean-build": "rm -rf build/*",
    "clean-lib": "rm -rf lib/*",
    "clean": "npm run clean-build && npm run clean-lib"
  },
  "keywords": [
    "chrome",
    "extensions",
    "options",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mrmisterman/chrome-extension-options"
  },
  "author": "Mike McCabe <mrmikemccabe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "yaml-loader": "^0.4.0"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1"
  }
}
